{
  "name": "cochain",
  "version": "0.0.1",
  "description": "Cochain Allows Users to Coedit Chain",
  "main": "server/index.js",
  "scripts": {
    "start": "node .",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.babel.js",
