    "cors": "^2.8.4",
    "fast-json-patch": "^2.0.4",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-codemirror": "^1.0.0",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-batched-actions": "^0.1.6",
    "socket.io": "^2.0.3",
