    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.5",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "react-hot-loader": "^1.3.1",
